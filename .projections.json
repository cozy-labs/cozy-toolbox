{
  "lib/web/models/*_model.ex": {
    "type": "model",
    "template": [
      "defmodule Web.Models.{basename|camelcase|capitalize} do",
      "end"
    ]
  },
  "lib/web/views/*_view.ex": {
    "type": "view",
    "template": [
      "defmodule Web.{basename|camelcase|capitalize}View do",
      "  use Web, :view",
      "end"
    ]
  },
  "lib/web/controllers/*_controller.ex": {
    "type": "controller",
    "template": [
      "defmodule Web.{basename|camelcase|capitalize}Controller do",
      "  use Web, :controller",
      "end"
    ]
  },
  "lib/web/channels/*_channel.ex": {
    "type": "channel",
    "template": [
      "defmodule Web.{basename|camelcase|capitalize}Channel do",
      "  use Web, :channel",
      "end"
    ]
  },
  "lib/*.ex": {
    "type": "source",
    "template": [
      "defmodule {camelcase|capitalize|dot} do",
      "end"
    ]
  }
}
